# Checks for optimizations
# Check whether --enable-debug was given.
if test "${enable_debug+set}" = set; then
  enableval=$enable_debug; ac_enable_debug=$enableval
else
  ac_enable_debug=no

fi

CONF_DEBUG=$ac_enable_debug


# Check whether --enable-test was given.
if test "${enable_test+set}" = set; then
  enableval=$enable_test; ac_enable_test=$enableval
else
  ac_enable_test=no

fi


# Check whether --enable-static was given.
if test "${enable_static+set}" = set; then
  enableval=$enable_static; ac_enable_static=$enableval
else
  ac_enable_static=no

fi


if test $ac_auto_cflags = yes; then
	if test $ac_enable_debug = yes ; then
		CFLAGS="-O0 -g -Wall -Wno-sign-compare -Wno-unused"
	elif test $ac_enable_test = yes ; then
		CFLAGS="-O0 -Wall -Wno-sign-compare -Wno-unused"
	else
		CFLAGS=""

{ echo "$as_me:$LINENO: checking whether ${CC-cc} accepts -march=native" >&5
echo $ECHO_N "checking whether ${CC-cc} accepts -march=native... $ECHO_C" >&6; }
echo 'void f(){}' > conftest.c
if test -z "`${CC-cc} -c -march=native conftest.c 2>&1`"; then
  { echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }
  CFLAGS="$CFLAGS -march=native"
else
  { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }

fi
rm -f conftest*



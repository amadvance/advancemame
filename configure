    {
      long int i = longval ();
      if (i != ((long int) (sizeof (char))))
	return 1;
      fprintf (f, "%ld", i);
    }
  else
    {
      unsigned long int i = ulongval ();
      if (i != ((long int) (sizeof (char))))
	return 1;
      fprintf (f, "%lu", i);
    }
